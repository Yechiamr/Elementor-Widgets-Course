protected function register_controls() {  //NOTICE- this function's closing bracket is at the bottom of the page!


//Content Tab with 2 sections

      $this->start_controls_section(
			'unique_section_name',
			      [
				      'label' => esc_html__( 'Section name on the panel', 'text_domain' ),
				      'tab' => \Elementor\Controls_Manager::TAB_CONTENT,
			      ]
	  	);

    	      // Add controls here

		  $this->end_controls_section();
    
      $this->start_controls_section(
			'unique_section_name',
			      [
				      'label' => esc_html__( 'Section name on the panel', 'text_domain' ),
				      'tab' => \Elementor\Controls_Manager::TAB_CONTENT,
			      ]
	  	);

    	      // Add controls here

		  $this->end_controls_section();
    
//Style Tab with 2 sections:

      $this->start_controls_section(
			'unique_section_name',
			      [
				      'label' => esc_html__( 'Section name on the panel', 'text_domain' ),
				      'tab' => \Elementor\Controls_Manager::TAB_STYLE,
			      ]
	  	);

    	      // Add controls here

		  $this->end_controls_section();
    
      $this->start_controls_section(
			'unique_section_name',
			      [
				      'label' => esc_html__( 'Section name on the panel', 'text_domain' ),
				      'tab' => \Elementor\Controls_Manager::TAB_STYLE,
			      ]
	  	);

	          // Add controls here

		$this->end_controls_section();
		
}
