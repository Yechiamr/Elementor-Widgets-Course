//Render two different text content controls

protected function render() {
                $settings = $this->get_settings_for_display();
                ?>
                    <div class="your-class"> 
                        <?php echo $settings['unique__control_name1']; ?>
                    </div>
                <?php
        }


// Render- style controls that effect one content control

protected function render() {
                $settings = $this->get_settings_for_display();
                ?>
                    <div class="your-class"> 
                        <?php echo $settings['unique___content_control_name']; ?>
                    </div>
                <?php
        }



// Content Template-  two different text content controls

        protected function content_template() {
                ?>
                    <div class="your-class"> 
                          <# 
                             {{ settings.unique_control_name1 }} 
                          #>
                    </div>
                <?php
        }


// Content Template- style controls that effect one content control


        protected function content_template() {
                ?>
                    <div class="your-class"> 
                          <# 
                             {{ settings.unique_content_control_name }} 
                          #>
                    </div>
                <?php
        }


